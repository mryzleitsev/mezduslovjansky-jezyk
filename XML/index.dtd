        <!ATTLIST img source CDATA #REQUIRED>
        <!ELEMENT linguists (linguist+)>
        <!ELEMENT linguist (#PCDATA)>
        <!ELEMENT content (paragraphs)>
        <!ELEMENT paragraphs (goal, idea, background, history, earlyProj, TwthCentury)>
        <!ELEMENT goal (#PCDATA)>
        <!ELEMENT idea (#PCDATA)>
        <!ELEMENT background (#PCDATA)>
        <!ELEMENT history (#PCDATA)>
        <!ELEMENT earlyProj (#PCDATA)>
        <!ELEMENT TwthCentury (#PCDATA)>